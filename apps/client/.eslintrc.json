{
    "extends": ["eslint:recommended", "next/core-web-vitals"],
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "off",
      "@next/next/no-html-link-for-pages": "off",
      "no-undef": "off"
    }
  }