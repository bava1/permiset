{
  "name": "permiset",
  "version": "1.4.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "apps/*",
    "tests/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
